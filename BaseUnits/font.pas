function font_get_pixel(FontIndex:byte;FontCharIndex:Byte;FontXPos,FontYPos:byte):boolean;
var FontPos,FontData:Dword;
    FontBit:byte;
begin
 FontPos:=(FontCharIndex-1)*10+1+((FontYPos-1)*16+FontXPos-1) div 32;
 FontData:=font_data[FontIndex,FontPos];
 FontBit:=((FontYPos-1)*16+FontXPos-1) mod 32;
 font_get_pixel:=(FontData shr FontBit) mod 2=1;
end;

end.

